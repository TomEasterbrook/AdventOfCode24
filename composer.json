{
  "name": "tomeasterbrook/advent-of-code-24",
  "description": "Advent of Code 2024",
  "version": "1.0.0",
  "require": {
    "php": ">=8.2",
    "illuminate/support": "^11.34",
    "laravel/prompts": "^0.3.2",
    "symfony/var-dumper": "^7.2"
  },
  "require-dev": {
    "pestphp/pest": "^3.5",
    "laravel/pint": "^1.18"
  },
  "autoload": {
    "psr-4": {
      "TomEasterbrook/AdventOfCode\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    },
    "config": {
      "allow-plugins": {
        "pestphp/pest-plugin": true
      }
    }
  },
  "config": {
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  }
}
